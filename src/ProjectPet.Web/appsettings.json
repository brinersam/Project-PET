{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Database": "Host=HOST;Port=PORT;Database=DBNAME;Username=UNAME;Password=PASS;",
    "Seq": "http://URL_FOR_SEQ:PORT"
  },

  "Minio": {
    "Endpoint": "projectpet.minio:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "WithSSL": false,
    "MaxConcurrentUpload": 5
  },

  "Db": {
    "CStringKey": "CStrings:Postgresql",
    "SoftDeletedCleanupFrequencyHours": "24",
    "SoftDeletedMaxLifeTimeDays": "30"
  },

  "OptionsTokens": {
    "GenerateTokens": true,
    "AccessExpiresMin": 10,
    "RefreshExpiresMin": 20
  },

  "Service.FileService": {
    "Endpoint": "http://projectpet.fileservice:6060"
  },

  "RabbitMq": {
    "Host": "amqp://projectpet.rabbitmq:5800",
    "Username": "guest",
    "Password": "guest"
  },

  "RedisOptions": {
    "Endpoint": "projectpet.fileservice.redis:4050"
  }
}
